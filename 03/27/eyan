function Named(firstname, Lastname) {
    var fullnamed = "My full name is" + firstname + " " + Lastname;

    if (Lastname == "Dumangon") {
        fullnamed = "MR. Dumangon bumangon welcum back";
    } else if (Lastname == "Tubo") {
        fullnamed = "Hi, Mr. Tubo wrong lulu "
    }
    return fullnamed;
}

var firstname = 'Ian Miguel';
var Lastname = 'Dumangon';

var fullnamed = Named(firstname, Lastname);

console.log(fullnamed);
console.log(Named(" Justine Tubo "));
console.log(Named(" Hans Dela Cruz "));
